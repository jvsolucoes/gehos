table.cadastro {
	height: 100%;
	width: 100%;	
}

table.cadastro table.campos {
}

.tdLista {
	background: #f7f7f7;
	border: 1px solid #d0d0d0;
	height: 100%;
}
.tdCampos {
	padding-left: 15px;
}
table.cadastro table.lista {
	width: 100%;
	height: 100%;
}
.cadTitulo {
	height: 30px;
	font-weight: bold;
	padding-left: 20px;
}
input {
	height: 22px;
	border: 1px solid #b0b0b0;
	font-size: 9pt;
	background: #ffffff;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.i1 {
	width: 150px;
}
.i25 {
	width: 329px;
}
.i3 {
	width: 410px;
}
.i5 {
	width: 670px;
}

.tn {
	width: 100px;
	text-align: right;
}
.ts {
	border-bottom: 1px dashed #aaa;
	margin-top: 17px;
	padding: 3px 0 3px 20px;
	font-weight: bold;
}
.pesquisa {
	width: 300px;
}

input:hover {
	border: 1px solid #00F;
}

.formCadastro {
	border: 0;
	border-spacing:: 0;
	padding: 0;
	margin: auto;
	margin-top: 10px;
}

.formCadastro > tbody > tr > td {
	font-size: 9pt;
	font-family: Verdana;
	color: #222;
	white-space: nowrap;
	text-align: left;
}
tr.dica > td {
	position: relative;
	font-size: 7pt;
	color: #b8b8b8;
	top: -5px;
	padding-bottom: 4px;
}
table.tbDica {
	width: 100%;
}
table.campos > tbody > tr > td {
	vertical-align: top;
}
.btnDel {
	background: url('../i/excluir.gif') top left no-repeat;
	height: 17px;
	width: 17px;
	cursor: pointer;
}



	